{
    "name": "dnunez24/craftcms-project",
    "description": "Craft CMS project starter",
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": "^7.0",
        "ext-imagick": "*",
        "ext-mbstring": "*",
        "ext-mcrypt": "*",
        "ext-redis": "*",
        "craftcms/cms": "2.*",
        "boboldehampsink/import": "^0.8",
        "boboldehampsink/export": "^0.5",
        "fromtheoutfit/navee": "^1.3.0",
        "nystudio107/retour": "^1.0.19",
        "nystudio107/seomatic": "^1.1.46",
        "madebykind/craft.labelenvironment": "^2.1.1",
        "pennebaker/craftcms-thearchitect": "1.5.5.3",
        "dnunez24/craft-laravel-mix": "*"
    },
    "require-dev": {
        "heroku/heroku-buildpack-php": "*",
        "squizlabs/php_codesniffer": "^2.8",
        "phpmd/phpmd": "^2.6"
    },
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/pixelandtonic/Twig"
    }, {
        "type": "vcs",
        "url": "https://github.com/pixelandtonic/guzzle3"
    }, {
        "type": "vcs",
        "url": "https://github.com/pixelandtonic/simplepie"
    }, {
        "type": "vcs",
        "url": "https://github.com/pixelandtonic/pel"
    }, {
        "type": "vcs",
        "url": "https://github.com/pixelandtonic/yii"
    }, {
        "type": "package",
        "package": {
            "name": "fromtheoutfit/navee",
            "type": "craft-plugin",
            "version": "1.3.0",
            "dist": {
                "url": "https://github.com/fromtheoutfit/navee/archive/v1.3.0.zip",
                "type": "zip"
            },
            "require": {
                "composer/installers": "~1.0"
            }
        }
    }, {
        "type": "package",
        "package": {
            "name": "madebykind/craft.labelenvironment",
            "type": "craft-plugin",
            "version": "2.1.1",
            "dist": {
                "url": "https://github.com/madebykind/craft.labelenvironment/archive/2.1.1.zip",
                "type": "zip"
            },
            "require": {
                "composer/installers": "~1.0"
            }
        }
    }, {
        "type": "path",
        "url": "./src/plugins/mix"
    }],
    "autoload": {
        "classmap": [
            "craft/vendor/craftcms/cms/src/"
        ]
    },
    "config": {
        "vendor-dir": "craft/vendor"
    },
    "scripts": {
        "post-install-cmd": [
            "bin/fix-plugins thearchitect environmentlabel"
        ],
        "post-update-cmd": [
            "bin/fix-plugins thearchitect environmentlabel"
        ]
    },
    "extra": {
        "installer-paths": {
            "craft/plugins/thearchitect/": ["pennebaker/craftcms-thearchitect"],
            "craft/plugins/environmentlabel/": ["madebykind/craft.labelenvironment"],
            "craft/plugins/mix/": ["dnunez24/craft-laravel-mix"]
        }
    }
}
