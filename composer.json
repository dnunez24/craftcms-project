{
  "name": "dnunez24/craftcms-project",
  "description": "Craft CMS project starter",
  "license": "MIT",
  "type": "project",
  "version": "dev-master",
  "minimum-stability": "beta",
  "require": {
    "php": ">=7.0.0",
    "craftcms/cms": "^3.0.0-beta.3",
    "craftcms/plugin-installer": "^1.0.0",
    "vlucas/phpdotenv": "^2.4.0",
